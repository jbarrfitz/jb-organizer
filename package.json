{
  "name": "jb-organizer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "sms:compile": "tsc -p ./tsconfig.twilio.json",
    "sms:copy-assets": "copyfiles sms/assets/* sms/assets/**/* --up 1 --exclude=\"**/*.ts\" static/",
    "sms:build": "yarn run sms:compile && yarn run sms:copy-assets",
    "sms:start": "yarn run sms:build && twilio-run --ngrok=\"\" --inspect=\"\"",
    "sms:deploy": "twilio-run deploy --environment=production -l debug",
    "sms:deploy:staging": "twilio-run deploy --environment=staging -l debug",
    "lint": "next lint",
    "test": "jest --runInBand --watch",
    "test:ci": "jest --ci",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "cypress:ci": "start-server-and-test dev http://localhost:3000 cypress"
  },
  "dependencies": {
    "@twilio/runtime-handler": "1.2.4",
    "bootstrap": "^5.2.0",
    "firebase": "^9.9.3",
    "next": "12.2.5",
    "react": "18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "18.2.0",
    "twilio": "^3.81.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@twilio-labs/serverless-runtime-types": "^2.2.3",
    "@types/node": "^18.7.13",
    "copyfiles": "^2.4.1",
    "cypress": "9",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-testing-library": "^5.6.0",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.0.1",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.9.1",
    "twilio-run": "3.4.4",
    "typescript": "^4.8.2"
  }
}
